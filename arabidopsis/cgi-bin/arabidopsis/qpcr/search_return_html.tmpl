<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
"http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <title>TIGR Arabidopsis thaliana Gene Expression</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link href="/arabidopsis/qpcr/qpcr.css" rel="stylesheet" type="text/css">
    <style type="text/css">

    <script src="/arabidopsis/qpcr/js/menu.js"></script>

    </style>


</head>

<body style="margin-left":50%;">
<div id="container" style="position:relative; width: 760px; ">

  <table border="1">
	<col>

 	<colgroup span="12" style="width:320px;"></colgroup>
    	<tr>
  		<th colspan="2"> Gene Name</th>
		<th colspan="1"> Tissue</th>
		<th colspan="3"> Cycle Time</th>
		<th colspan="2"> Standard Deviation <br>( <u>+</u> )</th>

		<th colspan="3"> Ratio to invariants</th>
		<th colspan="2"> Standard Deviation <br>( <u>+</u> )</th>
		<th colspan="3"> Absolute Concentration</th>
		<th colspan="2"> Standard Deviation <br>( <u>+</u> )</th>


    	</tr>

	<TMPL_LOOP NAME="singlesearch_table">
	   <tr>
  		<th colspan="2"> <TMPL_VAR NAME="elem_target_id"></th>
		<th colspan="1"> <TMPL_VAR NAME="elem_tissue"></th>
		<th colspan="3"> <TMPL_VAR NAME="elem_cycle_time"></th>
		<th colspan="2"> <TMPL_VAR NAME="elem_cycle_time2"></th>

		<th colspan="3"> <TMPL_VAR NAME="elem_ratio"></th>
		<th colspan="2"> <TMPL_VAR NAME="elem_ratio2"></th>
		<th colspan="3"> <TMPL_VAR NAME="elem_conc"></th>
		<th colspan="2"> <TMPL_VAR NAME="elem_conc2"></th>

	   </tr>
	</TMPL_LOOP>


</table>


</div> 
</body>
</html>

